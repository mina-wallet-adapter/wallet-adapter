{
  "name": "mina-wallet-adapter-ui-react",
  "version": "0.1.0",
  "author": "t4top",
  "license": "Apache-2.0",
  "repository": "https://github.com/aztemi/mina-wallet-adapter",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/wallet-adapter.css": "./dist/wallet-adapter.css"
  },
  "scripts": {
    "dev": "tsup src/index.tsx --format esm,cjs --dts --watch --external react",
    "build": "tsup src/index.tsx --format esm,cjs --dts --publicDir --external react",
    "clean": "shx rm -rf dist"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "shx": "^0.3.4",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
